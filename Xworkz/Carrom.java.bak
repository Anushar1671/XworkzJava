class Carrom{
	int price;
	String name;
	int noOfPlayer;
	String material;
	int coinColor;
	
	Carrom(int price,String name,int noOfPlayers,String material,int coinColor)
	{
		System.out.println("Invoking int,String,int,String,int in constructor");
