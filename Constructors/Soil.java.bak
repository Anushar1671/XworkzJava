class Soil{
	static String color;
	static String region;
	double pricePerLoad;
	String crop;
	
	
	Soil(String crop,double pricePerLoad)
	{
		System.out.println("Invoking String,double constructor of Soil");
		this.crop=crop;
		this.pricePerLoad=pricePerLoad;
	}
	
	static{
		color="Red";
		region="region";
	}
	
	static void printStatic()
	{
		System.out.println("Invoking printstatic in Soil");
		System.out.println("Soil color :"+color);
		System.out.println("Region :"+region);
	}
	
	void printInstance()
	{
		System.out.println("Invoking printInstance in Soil");
		System.out.println("Crop :"+crop);
		System.out.println("Soil price per load :"+pricePerLoad);
	}
}
